public class MyClass {
    public static void main(String args[]) {
      int x=9;
      int y=3;
      int i=-30;
      int a=10;
      int t=0;
      int o=9;
      int z=x*y;
      int p=i*a;
      int k=t*o;     

      System.out.println("PROD = " + z);
      System.out.println("PROD = " + p);
      System.out.println("PROD = " + k);
    }
}


