public class MyClass {
    public static void main(String args[]) {
      int x=10;
      int y=9;
      int i=-10;
      int a=4;
      int t=15;
      int o=-7;
      int z=x+y;
      int p=i+a;
      int k=t+o;
      

      System.out.println("X = " + z);
      System.out.println("X = " + p);
      System.out.println("X = " + k);
    }
}
